{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/components/*": ["./components/*"],
      "@/context/*": ["./components/Context/*"],
      "@/data/*": ["./data/*"],
      "@/pages/*": ["./components/pages/*"],
      "@/swiper/*": ["./components/Swipers/*"],
      "@/utils/*": ["./utils/*"],
      "@/API/*": ["./API/*"],
      "@/db/*": ["./db/*"],
      "@/models/*": ["./db/models/*"],
      "@/redux/*": ["./GlobalRedux/*"],
      "@/interface/*": ["./Interfaces/*"],





    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/Icons/Icons.js"],
  "exclude": ["node_modules"]
}
